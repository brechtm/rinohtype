[STYLESHEET]
name = Sphinx out-of-line flowables
base = sphinx_base14


[title page date : Paragraph('title page date')]
hide = true

[dedication section : Section(id='dedication')]
show_in_toc = false
page_break = right

[front matter section : Section(has_class='front-matter')]
show_in_toc = true
page_break = right

[front matter heading level 1 : 'front matter section' / Heading]
number_format = none

[table of contents section]
page_break = right

[epigraph : OutOfLineFlowables(name='epigraph')]
title = 'Epigraph'
page_break = right
