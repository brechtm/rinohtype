[TEMPLATE_CONFIGURATION]
name=
template=article
description=

stylesheet=sphinx_article
abstract_location=title

[STRINGS]
@important = 'IMPORTANT:'
@tip = 'TIP:'

[VARIABLES]
paper_size=A4


[contents]
page_number_format=number
end_at_page=any

[page]
page_size=$(paper_size)
left_margin=1in
right_margin=1in
top_margin=1in
bottom_margin=1in

[contents_page]
footer_text = '\t{PAGE_NUMBER} of {NUMBER_OF_PAGES}'
